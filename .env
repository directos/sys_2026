# Environment Variables for Vue.js

NODE_ENV=development
VUE_APP_VERSION=1.0.0

# Application
VUE_APP_NAME=Sys
VUE_APP_ENV=local
VUE_APP_DEBUG=true
VUE_APP_URL=http://localhost/sys/src/frontend

# API
VUE_APP_API_URL=http://localhost/sys/src/backend

# Authentication
VUE_APP_JWT_SECRET=your_jwt_secret_key
VUE_APP_JWT_EXPIRATION=3600

# Miscellaneous
VUE_APP_TIMEZONE=UTC
